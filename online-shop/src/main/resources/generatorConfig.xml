<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="testTables" targetRuntime="MyBatis3">
        <jdbcConnection
                driverClass="com.mysql.cj.jdbc.Driver"
                connectionURL="jdbc:mysql://localhost:3306/shop?useUnicode=true&amp;characterEncoding=UTF-8&amp;useSSL=false"
                userId="root"
                password="123456"
        ></jdbcConnection>
        <javaModelGenerator targetPackage="com.example.onlineshop.entity" targetProject="./src/main/java"></javaModelGenerator>
        <sqlMapGenerator targetPackage="com.example.onlineshop.mapper" targetProject="./src/main/java"></sqlMapGenerator>
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.example.onlineshop.mapper" targetProject="./src/main/java"></javaClientGenerator>
        <table tableName="address"></table>
        <table tableName="bankCard"></table>
        <table tableName="buyer"></table>
        <table tableName="comments"></table>
        <table tableName="coupon"></table>
        <table tableName="couponWarehouse"></table>
        <table tableName="favourite"></table>
        <table tableName="goods"></table>
        <table tableName="order"></table>
        <table tableName="seller"></table>
        <table tableName="shoppingCart"></table>
    </context>
</generatorConfiguration>